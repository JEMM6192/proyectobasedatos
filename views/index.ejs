                            <a href="detail-page.html" class="custom-block-image-wrap">
                                <a href="/mantenimientos"><img src="https://i.ibb.co/YRzG2g7/reparacion-de-autos.png"
                                    class="custom-block-image img-fluid bi-align-center" alt="">
                            </a>

                            <div class="custom-block-info custom-block-overlay-info">
                                <h5 class="mb-1">
                                    <a href="listing-page.html"> Mantenimientos</a>
                                </h5>
                           <p class="badge mb-0"><%= mantenimientos[0].mantenimientos %></p>
                            </div>
                        </div>
                    </div>


                    <div class="col-lg-3 col-md-6 col-12 mb-4 mb-lg-0 text-lg-center">
                        <div class="custom-block custom-block-overlay align-content-center">
