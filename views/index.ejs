                    </div>
                </div>
            </div>
        </nav>


        <section class="hero-section">
            <div class="container">
                <div class="row">

                    <div class="col-lg-12 col-12">
                        <div class="text-center mb-5 pb-2">
                            <h1 class="text-white">En Mantenimiento</h1>

                        </div>


                        <div class="owl-carousel owl-theme">
                            
                            <% Enprogreso.forEach(function (Enprogreso) { %>

                            <div class="owl-carousel-info-wrap  ">
                                  <div class="col">
                                    <div class="card h-100 text-bg-light">
                                      <img src="<%= Enprogreso.FotoRuta %>" class="card-img-top" alt="...">
                                      <div class="card-body">
                                        <p class="card-text">This content is a little bit longer.</p>
                                      </div>
                                      <div class="card-footer">
                                        <small class="text-muted">-----------------------</small>
                                      </div>
                                    </div>

                                    <div class="owl-carousel-info">
                                <h4 class="mb-2 text-center"><%= Enprogreso.Marca %></h4>
                                    
                         </div>
                             </div> 
                            </div>
                            <% }); %>

                        </div>
                    </div>

                </div>
            </div>
        </section>


        
